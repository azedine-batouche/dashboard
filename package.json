{
  "name": "dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false --progress=false ",
    "lint": "ng lint",
    "e2e": "ng e2e --protractor-config=./e2e/protractor-ci.conf.js",
    "prettier": "prettier --write '**/*.{ts,scss,md}'",
    "cc": "npm run prettier &&  npm run lint",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build:gh-pages:test1": "ng build -c=test --output-path=gh-pages/test1/ --base-href=/dashboard/test1/",
    "pretty-quick": "pretty-quick",
    "pwa": "http-server ./gh-pages/test1 -p 9005"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "bootstrap": "^4.4.1",
    "bourbon": "^6.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.4.0",
    "serialize-javascript": "^2.1.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "autoprefixer": "^ 6.5.4 ",
    "codelyzer": "^5.0.0",
    "coveralls": "^3.0.9",
    "husky": "^4.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "autoprefixer": "^ 6.5.4 "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "npm run pretty-quick --staged && npm run lint"
    }
  }
}
